',\r\n    outline: 'none',\r\n}\r\n\r\n\r\nconst SearchIcon = () => {\r\n    return(\r\n        <button type=\"submit\" className=\"search_Icon\" style={searchIconOptions}><img src={searchIcon} width='120%' height='38px'></img></button>\r\n    )\r\n}\r\n\r\nexport default SearchIcon;","C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\Body.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\Navi.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\HeaderComponents\\views\\Cart.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\Books.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\Best100.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\Clothes.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\TodayHot.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\Electronics.js",[],"C:\\Users\\xhens\\Desktop\\NEW\\react_mall\\MALL-with-react-boot\\frontend\\src\\layout\\component\\MainViewComponents\\views\\components\\ItemPages.js",["155","156","157"],"import React, {Component, useState, useEffect} from 'react';\r\nimport styled from 'styled-components';\r\nimport axios from 'axios';\r\nimport '../../../../css/itemPages.css';\r\n\r\n\r\nconst Wrapper = styled.div`\r\n    height: 1020px;\r\n    width: 100%;\r\n    top: 60px;\r\n    z-index: 5;\r\n\r\n    color: black;\r\n    border-top: 3px solid black;\r\n    font-size: 25px;\r\n    min-width: 1300px;\r\n\r\n    padding: 10px 60px;\r\n`;\r\n\r\n\r\nconst Rule = ({ color })